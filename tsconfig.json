{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src"],
    "types": [],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "es6",
    "lib": ["dom", "es2017"],
    "module": "esnext",
    "jsx": "react-jsx",
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "include": ["vite-env.d.ts", "src/**/*"]
}
